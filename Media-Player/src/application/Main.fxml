<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Text?>

<BorderPane onKeyPressed="#playOrStop" prefHeight="805.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <top>
      <MenuBar minHeight="25.0" prefHeight="25.0" scaleShape="false" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="openFile" mnemonicParsing="false" onAction="#openFile" text="Open File" />
             </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Donate">
             <items>
               <MenuItem mnemonicParsing="false" onAction="#donate" text="Go to webpage" />
             </items>
           </Menu>
        </menus>
      </MenuBar>
   </top>
   <bottom>
      <StackPane minHeight="-Infinity" prefHeight="60.0" prefWidth="1280.0" BorderPane.alignment="CENTER">
         <children>
            <HBox fx:id="hbox" alignment="CENTER" maxHeight="30.0" minHeight="-Infinity" prefHeight="50.0" scaleShape="false">
               <children>
                  <Slider fx:id="timeSlider" prefHeight="14.0" prefWidth="705.0" />
                  <Text fx:id="timeElapsed" strokeType="OUTSIDE" strokeWidth="0.0" text="00:00" />
                  <Button fx:id="play" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#play" prefHeight="35.0" prefWidth="35.0" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets left="30.0" right="10.0" />
                     </HBox.margin>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </Button>
                  <Button fx:id="pause" mnemonicParsing="false" onAction="#pause" prefHeight="35.0" prefWidth="35.0">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="stop" mnemonicParsing="false" onAction="#stop" prefHeight="35.0" prefWidth="35.0">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="fast" mnemonicParsing="false" onAction="#fastForward" prefHeight="35.0" prefWidth="35.0">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="slow" mnemonicParsing="false" onAction="#slowDown" prefHeight="35.0" prefWidth="35.0" >
                     <HBox.margin>
                        <Insets right="40.0" />
                     </HBox.margin>
                  </Button>
                  <Slider fx:id="volumeSlider">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Slider>
               </children>
            </HBox>
         </children>
      </StackPane>
   </bottom>
   <center>
      <StackPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" BorderPane.alignment="TOP_LEFT">
         <children>
            <MediaView fx:id="mediaView" onKeyPressed="#playOrStop">
               <viewport>
                  <Rectangle2D />
               </viewport>
            </MediaView>
            <MediaView fx:id="mediaView1" layoutX="650.0" layoutY="370.0" onKeyPressed="#playOrStop">
               <viewport>
                  <Rectangle2D />
               </viewport>
            </MediaView>
         </children>
      </StackPane>
   </center>
</BorderPane>
